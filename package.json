    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "react-test-renderer": "^16.6.3",
    "rimraf": "^2.6.2",
    "serve": "^10.1.1"
  }
